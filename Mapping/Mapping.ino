void setup() {
  // put your setup code here, to run once:
  ws2812b.begin();

  Serial.begin(115200);

    // Create the BLE Server
    BLEDevice::init("LiteMonitr");
    pServer = BLEDevice::createServer();
    pServer->setCallbacks(new MyServerCallbacks());

    // Create the BLE Service
    BLEService *pService = pServer->createService("4fafc201-1fb5-459e-8fcc-c5c9c331914b");

    // Create a BLE Characteristic
    pCharacteristic = pService->createCharacteristic(
                        "beb5483e-36e1-4688-b7f5-ea07361b26a8",
                        BLECharacteristic::PROPERTY_READ |
                        BLECharacteristic::PROPERTY_WRITE |
                        BLECharacteristic::PROPERTY_NOTIFY
                    );

    pCharacteristic->setCallbacks(new MyCharacteristicCallbacks());

    // Start the service
    pService->start();

    // Start advertising
    BLEAdvertising *pAdvertising = BLEDevice::getAdvertising();
    pAdvertising->addServiceUUID(SERVICE_UUID);
    pAdvertising->setScanResponse(true);
    pAdvertising->setMinPreferred(0x06);  // functions that help with iPhone connections issue
    pAdvertising->setMinPreferred(0x12);
    BLEDevice::startAdvertising();
    Serial.println("Characteristic defined! Now you can read it in your phone!");
}

int option = 1;

void loop() {
  // put your main code here, to run repeatedly:
  if (deviceConnected) {
        // Your main code when a device is connected

        // Check if there's new data available
        if (newDataAvailable) {
          if(value.length() == 1){
            Serial.println("Received value: " + String(receivedValue));

            int preOption = String(receivedValue).toInt();

            if(preOption == 6){
              brightness -= .05;
              if(brightness < 0) brightness = 0;
            }else if(preOption == 7){
              brightness += .05;
              if(brightness > 1) brightness = 1;
            }else{
              option = String(receivedValue).toInt();
            }

            if(option == 0){ 
              ws2812b.clear();
              ws2812b.show();
            }

            switch(option){
              case 1:
                draw64x32(testimage);
                break;
              case 2:
                draw64x32(mariospritetest);
                break;
              case 3:
                draw64x32(pacman);
                break;
              case 4:
                draw64x32(polkadots);
                break;
              case 5:
                draw64x32(stickman);
                break;
            }
          }else if(value.length() == 20 && value[0] == 37){
            uint8_t R = value[1];
            uint8_t G = value[2];
            uint8_t B = value[3];

            uint8_t rowIdx = 4;
            uint8_t colIdx = 5;

            // Serial.println("drawing (" + String(R) + ", " + String(G) + ", " + String(B) + " ) @ " + String(row) + ", " + String(col));

            while(value[rowIdx] < 32 && value[colIdx] < 64 && rowIdx < 20 && colIdx < 20) {

              setSinglePixel(value[rowIdx], value[colIdx], R, G, B);
              Serial.println("drawing (" + String(R) + ", " + String(G) + ", " + String(B) + " ) @ " + String(value[rowIdx]) + ", " + String(value[colIdx]));

              rowIdx += 2;
              colIdx += 2;

              
            }

            ws2812b.show();

          }
          // Reset the flag
            newDataAvailable = false;
        }
    }
    delay(5);
}